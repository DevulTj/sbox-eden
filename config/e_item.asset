@BaseClass = e_item : ""
[
	itemtype(choices) { group = "Meta" } : "Item Type" : 0 : "" =
	[
		0 : "Item"
		1 : "Food"
		2 : "Deployable"
		3 : "Tool"
	]
	itemname(string) { group = "Meta" } : "Item Name" :  : ""
	itemdescription(string) { group = "Meta" } : "Item Description" :  : ""
	iconpath(string) { group = "Meta" editor = "AssetBrowse(png)" } : "Icon Path" :  : ""
]

