//
// Eden.ResourceItemQuantity
//
@struct = Eden_ResourceItemQuantity
[
	itemasset(string) { editor = "AssetBrowse(e_item)" } : "Item Asset" :  : ""
	totalquantity(string) : "Total Quantity" :  : ""
]

//
// Eden.ResourceAsset
//
@BaseClass = e_resource : ""
[
	resourcename(string) { group = "Meta" } : "Resource Name" :  : ""
	worldmodelpath(resource:vmdl) { group = "World" } : "World Model Path" :  : ""
	resourcetype(choices) { group = "Gathering" } : "Resource Type" : 0 : "" =
	[
		0 : "Wood"
		1 : "Stone"
	]
	yieldperhit(integer) { group = "Gathering" } : "Yield Per Hit" : 0 : ""
	itemstogather(array:struct:Eden_ResourceItemQuantity) { group = "Gathering" } : "Items To Gather" :  : ""
]

